{
    "name": "sped-nfsginfe",
    "type": "library",
    "description": "Api para comunicação com prefeituras que usando o padrão GINFE",
    "keywords": ["nfs","sped","nfephp"],
    "homepage": "https://github.com/Focus599Dev/sped-nfsginfe",
    "license": ["LGPL-3.0-or-later", "GPL-3.0-or-later", "MIT"],
    "authors": [
        {
            "name": "Marlon Oliveira Barbosa",
            "email": "marlon.academi@gmail.com",
            "homepage": "https://github.com/Focus599Dev/sped-nfsginfe",
            "role": "Developer"
        },
    ],
    "require": {
        "php" : ">= 7.0",
        "nfephp-org/sped-common" : "^5.0",
        "justinrainbow/json-schema": "^5.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^5.7",
        "scrutinizer/ocular": "^1.3",
        "phpmd/phpmd": "dev-master",
        "sebastian/phpcpd": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "NFePHP\\NFSe\\GINFE\\": "src/"
        } 
    },
    "autoload-dev": {
        "psr-4": {
            "NFePHP\\NFSe\\GINFE\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist",
        "phpcbf": "vendor/bin/phpcbf --standard=psr2 src; vendor/bin/phpcbf --standard=psr2 tests",
        "phpcs": "vendor/bin/phpcs --standard=psr2 src; vendor/bin/phpcs --standard=psr2 tests"
    },
    "extra": {
        "branch-alias": {
            "v1.0": "master"
        }
    },
    "minimum-stability": "stable"
}
